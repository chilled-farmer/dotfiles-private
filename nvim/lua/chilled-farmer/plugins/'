--download lazy vim if it is not installed
local lazypath = vim.fn.stdpath("data") .. "/lazy/lazy.nvim"
local vimDefaults=require("chilled-farmer.options")
local keymaps=require("chilled-farmer.keymaps")
if not (vim.uv or vim.loop).fs_stat(lazypath) then
  vim.fn.system({
    "git",
    "clone",
    "--filter=blob:none",
    "https://github.com/folke/lazy.nvim.git",
    "--branch=stable", -- latest stable release
    lazypath,
  })
end
vim.opt.rtp:prepend(lazypath)
require("lazy").setup(
{
"folke/which-key.nvim",
{import="chilled-farmer.plugins.lazy"},
{import="chilled-farmer.plugins.telescope"},
{import="chilled-farmer.plugins.treesitter"},
{import="chilled-farmer.plugins.harpoon"},
{import="chilled-farmer.plugins.undo-tree"},
},{
vimDefaults,
keymaps
})
